{
  "name": "react-hooks-boilerplate",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "vite build src --config ./config/vite/vite.config.js",
    "dev": "vite src --config ./config/vite/vite.config.js",
    "preview": "vite preview dist --config ./config/vite/vite.config.js",
    "lint": "eslint -c ./config/eslint/.eslintrc 'src/**/*.{ts,tsx}' --ignore-pattern '*.test.*'",
    "start": "serve -l 3000 -s dist",
    "test": "vitest --config ./config/vite/test.config.js",
    "test:coverage": "vitest --config ./config/vite/test.config.js --coverage",
    "prettier:write": "prettier --config ./config/prettier/.prettierrc --write ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deejaygeroso/react-hooks-boilerplate.git"
  },
  "author": "Deejay Geroso",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/deejaygeroso/react-hooks-boilerplate/issues"
  },
  "homepage": "https://github.com/deejaygeroso/react-hooks-boilerplate#readme",
  "devDependencies": {
    "@react-helper/eslint-config": "^1.0.0",
    "@react-helper/prettier-config": "^1.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "^7.32.0",
    "jsdom": "^21.1.0",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vitest": "^0.28.5"
  },
  "dependencies": {
    "path": "^0.12.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1"
  }
}
